<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="676.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Employee">
    <children>
        <Text layoutX="33.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Manage Member">
            <font>
                <Font size="24.0" />
            </font>
        </Text>
        <ButtonBar layoutX="14.0" layoutY="88.0" prefHeight="42.0" prefWidth="294.0">
            <buttons>
                <Button fx:id="addMember" mnemonicParsing="false" onAction="#handle" prefHeight="35.0" text="ADD" />
                <Button fx:id="editMember" mnemonicParsing="false" onAction="#handle" prefHeight="35.0" text="EDIT" />
                <Button fx:id="deleteMember" minWidth="45.0" mnemonicParsing="false" onAction="#handle" prefHeight="35.0" prefWidth="89.0" text="DELETE" />
            </buttons>
        </ButtonBar>
        <Text layoutX="37.0" layoutY="186.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Mange Classes">
            <font>
                <Font size="24.0" />
            </font>
        </Text>

        <ButtonBar layoutX="9.0" layoutY="313.0" prefHeight="42.0" prefWidth="583.0">
            <buttons>
                <Button fx:id="viewAllMemInfo" mnemonicParsing="false" onAction="#handle" prefHeight="35.0" text="All members Info" textAlignment="RIGHT" />
                <Button fx:id="viewSpecClass" mnemonicParsing="false" onAction="#handle" prefHeight="35.0" prefWidth="120.0" text="by specific class" />
                <Button fx:id="viewSpecMemShip" minWidth="45.0" mnemonicParsing="false" onAction="#handle" prefHeight="35.0" prefWidth="206.0" text="under specific membership" textAlignment="CENTER" />
            </buttons>
        </ButtonBar>
        <MenuBar layoutY="2.0" prefHeight="26.0" prefWidth="676.0" />
        <Button fx:id="logoutBtn" layoutX="606.0" layoutY="2.0" mnemonicParsing="false" onAction="#handle" text="Logout" />
    </children>
</AnchorPane>
